{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "rules": {
    "interface-name": false,
    "member-ordering": false,
    "no-angle-bracket-type-assertion": false,
    "member-access": false,
    "jsx-no-lambda": false,
    "object-literal-sort-keys": false,
    "no-shadowed-variable": false
  },
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  }
}
